{
  "manifest_version": 2,
  "version": "4.0.6.485",
  "name": "RingCentral for Google",
  "description": "RingCentral for Google",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "notifications",
    "tabCapture",
    "http://*/",
    "https://*/",
    "storage",
    "identity"
  ],
  "oauth2": {
    "client_id": "420036030335-fueai0i5mpo6lrfqlekj1p1t0jbj78qq.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.google.com/m8/feeds/",
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/plus.profile.emails.read",
      "https://www.googleapis.com/auth/admin.directory.user.readonly"
    ]
  },
  "browser_action": {},
  "icons": {
    "16": "rc16.png",
    "32": "rc32.png",
    "48": "rc48.png",
    "128": "rc128.png"
  },
  "web_accessible_resources": [
    "redirect.html",
    "client.html"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' https://*.googleapis.com https://*.google.com https://*.segment.com https://*.mxpnl.com; object-src 'self'",
  "options_ui": {
    "page": "options.html"
  },
  "key": ""
}